<div id="<%= dom_id article %>">

  <div>
    <p>
      <strong>Title:</strong>
      <%= article.title %>
    </p>
 
    <p>
      <strong>Body:</strong>
      <%= article.body %>
    </p>
  <div>

  <div>
    <p class="article_details">
      <strong>Created at:</strong>
      <%= article.created_at %>
    </p>
    <p class="article_details">
      <strong>Updated at:</strong>
      <%= article.updated_at %>
    </p>
  </div>

</div>
