<div id="<%= dom_id article %>">

  <div>
    <p dom_id="article_title">
      <strong>Title:</strong>
      <%= article.title %>
    </p>
  <div>

  <div>
    <p dom_id="article_description">
      <strong>Body:</strong>
      <%= article.body %>
    </p>
  <div>

  <div>
    <p dom_id="article_created_at">
      <strong>Created at:</strong>
      <%= article.created_at %>
    </p>
    <p dom_id="article_updated_at">
      <strong>Updated at:</strong>
      <%= article.updated_at %>
    </p>
  </div>

</div>
